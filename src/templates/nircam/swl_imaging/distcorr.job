#
# DISTCORR template.
#
Executable              = {{ code_root }}/dummyProcess.py
Arguments               = -i {{ dataset }}_telcorr_$(Process).fits \
                          -o {{ dataset }}_distcorr_$(Process).fits
Output                  = processSif_{{ dataset }}.out
Error                   = processSif_{{ dataset }}.err

Universe                = vanilla
Log                     = {{ dataset }}.log
+InputDataset           = "{{ dataset }}"

Should_Transfer_Files   = ALWAYS
When_to_Transfer_Output = ON_EXIT
transfer_input_files    = {{ dataset }}_telcorr_$(Process).fits

+HookKeyword            = "OWL"
+Instances              = {{ num_ccds }}

Notification            = Never
Queue {{ num_ccds }}
